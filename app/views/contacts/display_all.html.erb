<h1>All Contacts:</h1>

<table>
  <th>
    <td><strong>First Name</strong></td>
    <td>Last Name</td>
    <td>Email</td>
    <td>Phone</td>
  </th>
<% @contacts.each do |contact| %>
  <tr>
    <td><%= contact.first_name %></td>
    <td><%= contact.last_name %></td>
    <td><%= contact.email %></td>
    <td><%= contact.phone %></td>
  </tr>
<% end %>
</table>